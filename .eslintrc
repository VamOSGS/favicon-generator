{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": 0,
    "linebreak-style": 0,
    "react/prop-types": 0,
    "no-return-assign": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/click-events-have-key-events": 0
  }
}
